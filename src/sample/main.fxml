<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<VBox fx:id="container" prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <AnchorPane maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
     <children>
        <Label alignment="CENTER" layoutX="155.0" layoutY="177.0" style="&#10;" text="Drag components from Library hereâ€¦" textAlignment="CENTER" textFill="#9f9f9f" wrapText="false">
           <font>
              <Font size="18.0" />
           </font>
        </Label>
        <VBox layoutX="-1.0" layoutY="1.0" prefHeight="400.0" prefWidth="640.0" style="-fx-background-color: #364896;">
           <children>
              <Label alignment="CENTER" prefHeight="72.0" prefWidth="640.0" style="-fx-background-color: #364896;" text="Pomodoro Clock" textAlignment="CENTER" textFill="#f8f6f6">
                 <font>
                    <Font name="Georgia" size="29.0" />
                 </font>
              </Label>
              <HBox prefHeight="150.0" prefWidth="640.0">
                 <children>
                    <Pane prefHeight="131.0" prefWidth="443.0">
                       <children>
                          <Label alignment="CENTER" contentDisplay="CENTER" layoutX="113.0" layoutY="2.0" prefHeight="25.0" prefWidth="144.0" text="Session Length" textFill="#fcf6f6">
                             <font>
                                <Font name="Bodoni MT" size="20.0" />
                             </font>
                          </Label>
                          <Label fx:id="sessionLength" alignment="CENTER" contentDisplay="CENTER" layoutX="145.0" layoutY="32.0" prefHeight="92.0" prefWidth="81.0" text="25" textAlignment="CENTER" textFill="#fff8f8">
                             <font>
                                <Font name="Comic Sans MS" size="53.0" />
                             </font>
                          </Label>
                          <Button fx:id="addSessionLength" contentDisplay="CENTER" graphicTextGap="0.0" layoutX="63.0" layoutY="39.0" mnemonicParsing="false" onAction="#increaseSessionLength" prefHeight="76.0" prefWidth="81.0" style="-fx-background-color: inherit;" text="+" textAlignment="CENTER" textFill="#fff9f9">
                             <font>
                                <Font size="36.0" />
                             </font>
                             <effect>
                                <DropShadow />
                             </effect>
                          </Button>
                          <Button fx:id="subtractSessionLength" contentDisplay="CENTER" graphicTextGap="0.0" layoutX="225.0" layoutY="37.0" mnemonicParsing="false" onAction="#decreaseSessionLength" prefHeight="82.0" prefWidth="81.0" style="-fx-background-color: inherit;" text="-" textAlignment="CENTER" textFill="#fff9f9">
                             <font>
                                <Font size="38.0" />
                             </font>
                             <effect>
                                <DropShadow />
                             </effect>
                          </Button>
                       </children>
                    </Pane>
                    <Pane prefHeight="131.0" prefWidth="454.0">
                       <children>
                          <Label layoutX="122.0" layoutY="2.0" text="Break Length" textFill="#fffdfd">
                             <font>
                                <Font name="Bodoni MT" size="20.0" />
                             </font>
                          </Label>
                          <Label fx:id="breakLength" alignment="CENTER" contentDisplay="CENTER" layoutX="149.0" layoutY="44.0" prefHeight="56.0" prefWidth="62.0" text="5" textAlignment="CENTER" textFill="#f8f4f4">
                             <font>
                                <Font name="Comic Sans MS" size="50.0" />
                             </font>
                          </Label>
                          <Button fx:id="subtractBreakLength" contentDisplay="CENTER" graphicTextGap="0.0" layoutX="211.0" layoutY="38.0" mnemonicParsing="false" onAction="#decreaseBreakLength" prefHeight="82.0" prefWidth="81.0" style="-fx-background-color: inherit;" text="-" textAlignment="CENTER" textFill="#fff9f9">
                             <font>
                                <Font size="39.0" />
                             </font>
                             <effect>
                                <DropShadow />
                             </effect>
                          </Button>
                          <Button fx:id="addBreakLength" contentDisplay="CENTER" graphicTextGap="0.0" layoutX="61.0" layoutY="38.0" mnemonicParsing="false" onAction="#increaseBreakLength" prefHeight="82.0" prefWidth="81.0" style="-fx-background-color: inherit;" text="+" textAlignment="CENTER" textFill="#fff9f9">
                             <font>
                                <Font size="39.0" />
                             </font>
                             <effect>
                                <DropShadow />
                             </effect>
                          </Button>
                       </children>
                    </Pane>
                 </children>
              </HBox>
              <Pane prefHeight="222.0" prefWidth="640.0">
                 <children>
                    <Label fx:id="timerRunning" alignment="CENTER" contentDisplay="CENTER" layoutX="186.0" layoutY="18.0" prefHeight="136.0" prefWidth="285.0" text="25:00" textFill="#fcfafa">
                       <font>
                          <Font name="Californian FB" size="96.0" />
                       </font>
                    </Label>
                    <ButtonBar layoutX="182.0" layoutY="145.0" prefHeight="40.0" prefWidth="285.0">
                       <buttons>
                          <Button fx:id="playButton" mnemonicParsing="false" onAction="#play" style="-fx-background-color: #fff;" text="PLAY">
                             <font>
                                <Font name="Comic Sans MS Bold" size="12.0" />
                             </font>
                          </Button>
                          <Button fx:id="pauseButton" mnemonicParsing="false" style="-fx-background-color: #fff;" text="PAUSE" onAction="#pauseCounter">
                             <font>
                                <Font name="Comic Sans MS Bold" size="12.0" />
                             </font>
                          </Button>
                          <Button fx:id="restartButton" mnemonicParsing="false" prefHeight="26.0" prefWidth="105.0" style="-fx-background-color: #fff;" text="RESTART" onAction="#reset">
                             <font>
                                <Font name="Comic Sans MS Bold" size="12.0" />
                             </font>
                          </Button>
                       </buttons>
                    </ButtonBar>
                 </children>
              </Pane>
           </children>
           <effect>
              <DropShadow />
           </effect>
           <opaqueInsets>
              <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
           </opaqueInsets>
        </VBox>
     </children>
   </AnchorPane>
</VBox>
